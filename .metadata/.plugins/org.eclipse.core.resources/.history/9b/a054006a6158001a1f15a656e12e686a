
public class demo {

	public static void main(String[] args) {
		
		getLargestPrime(7);
		
	}
	
	public static int getLargestPrime(int number) {
		if(number<=1) {
			return -1;
		}
		int count = 0;
		int temp = 0;
		for(int i =1; i<number; i++) {
			if(number%i ==0) {
				count++;
				temp = i;
			}
		}
		//System.out.println(count);
		//System.out.println(temp);
		
		if(count==2) {
			System.out.println(number);
		}else {
			System.out.println(temp);	
		}
			return 0;
		
		
	}
	
}
