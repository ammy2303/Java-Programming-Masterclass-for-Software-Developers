
public class demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int getLargestPrime(int number) {
		
		int count = 0;
		int temp = 0;
		int copy = number;
		
		if(number<=1) {
			return -1;
		}
		
		for(int i = 0; i< number/2; i++) {
			if(number%i == 0) {
				count ++;
				temp = i;
			}
		}
		if(count < 1) {
			return -1;
		}
		return temp;
		
			
	}
	
}
