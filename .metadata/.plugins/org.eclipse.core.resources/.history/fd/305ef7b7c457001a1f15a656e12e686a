
public class demo {

	public static void main(String[] args) {
		
		numberToWords(4556);
		
	}
	
	public static void numberToWords(int number) {
		if(number<0) {
			System.out.println("Invalid Value");
		}
		int remainder = 0;
		while(number != 0) {
			remainder = number % 10;
			number = number / 10;
			switch(remainder) {
			case 0:
				System.out.println("ONE");
				break;
			case 1:
				System.out.println("ONE");
				break;
			case 2:
				System.out.println("ONE");
				break;
			case 3:
				System.out.println("ONE");
				break;
			case 4:
				System.out.println("ONE");
				break;
			case 5:
				System.out.println("ONE");
				break;
			case 6:
				System.out.println("ONE");
				break;
			case 7:
				System.out.println("ONE");
				break;
			case 8:
				System.out.println("ONE");
				break;
			case 9:
				System.out.println("ONE");
				break;
			default:
				System.out.println("Exception");
				break;	
			
			}
		}
		
	}

}
